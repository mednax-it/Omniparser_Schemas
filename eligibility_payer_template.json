{
  "parser_settings": {
    "version": "omni.2.1",
    "file_format_type": "csv2",
    "debug": 1
  },
  "file_declaration": {
    "delimiter": ",",
    "records": [
      {
        "header": "^Payer,Templates$",
        "min": 1,
        "max": 1
      },
      {
        "max": -1,
        "is_target": true,
        "columns": [
          {
            "name": "payer",
            "index": 1
          },
          {
            "name": "templates",
            "index": 2
          }
        ]
      }
    ]
  },
  "transform_declarations": {
    "FINAL_OUTPUT": {
      "object": {
        "line_num": {
          "xpath": "__debug/line_num"
        },
        "payer": {
          "xpath": "payer"
        },
        "templates": {
          "xpath": "templates"
        }
      }
    }
  }
}
