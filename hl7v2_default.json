        }
    },
    "transform_attending_physician_identifier": {
        "custom_func": {
            "name": "javascript",
            "args": [
                {"const": "npi == '' ? '' : 'Provider/' + npi"},
                {"const": "npi"}, {"xpath": "attending_physician_id"},
                {"const": "lname"}, {"xpath": "attending_physician_last_name"},
