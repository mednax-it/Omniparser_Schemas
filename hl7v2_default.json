        "fullUrl": {"const": "urn:uuid:11ebe121-cdef-4613-9cf2-983a382cdef7"},
        "resource": {
          "object": {
            "resourceType": {"const": "RelatedPerson"},
            "identifier": {
              "array": [
                {
                  "object": {
                    "system": {"const": "http://hl7.org/fhir/sid/us-ssn"},
                    "value": {"const": "ssn9999"},
                    "type": {
                      "object": {
                        "coding": {
                          "array": [
                            {
                              "object": {
                                "system": {"const": "https://terminology.hl7.org/5.0.0/CodeSystem-v2-0203"},
