                {"const": "insured_lname !== null && insured_fname !== null ? insured_fname + ' ' + insured_lname : ''"},
                {"const": "insured_lname"}, {"xpath": "IN1/insured_last_name"},
                {"const": "insured_fname"}, {"xpath": "IN1/insured_first_name"}
            ]
        }
